# Example .env for scaffold
# Ollama: if Ollama runs locally with default settings no auth needed; otherwise set OLLAMA_URL
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=mixtral:8x7b

# OpenMemory (self-hosted) server URL
OPENMEMORY_URL=http://localhost:8765

# Optionally, if you use an API key for OpenMemory or Mem0 hosted platform
OPENMEMORY_API_KEY=

# Local LLM client keys (if mixing providers)
OPENAI_API_KEY=

# LLM Parameters
LLM_TEMPERATURE=0.1
LLM_MAX_TOKENS=512